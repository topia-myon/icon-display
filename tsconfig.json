{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  }
}
